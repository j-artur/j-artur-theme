    "tab.activeBackground": "#111316",
    "tab.activeForeground": "#A3ABB8",
    "tab.border": "#111316",
    "tab.activeBorder": "#3b82f6",
    "tab.inactiveBackground": "#111316",

    "debugToolBar.background": "#111316",
    "descriptionForeground": "#9298A3",
    "dropdown.background": "#111316",
    "dropdown.border": "#111316",
    "editor.background": "#111316",
    "editor.foreground": "#8691A2",
    "editor.findMatchBackground": "#2F343C",
    "editor.findMatchBorder": "#3b82f6",
    "editor.findMatchHighlightBackground": "#2F343C",
    "editor.lineHighlightBackground": "#1C1F26",
    "editorLineNumber.activeForeground": "#9298A3",

    "editor.selectionBackground": "#262A31",
    "editor.selectionHighlightBackground": "#E5E5E510",
    "editor.selectionHighlightBorder": "#C1C1C1",
    "editor.wordHighlightBackground": "#d2e0ff2f",
    "editor.wordHighlightBorder": "#7f848e",
    "editor.wordHighlightStrongBackground": "#9298A326",
    "editor.wordHighlightStrongBorder": "#7f848e",
    "editorBracketMatch.background": "#3A404C",
    "editorBracketMatch.border": "#3A404C",
    "editorCursor.background": "#E5E5E5c9",
    "editorCursor.foreground": "#0284c7",
    "editorError.foreground": "#A83630",
    "editorGroup.border": "#181a1f",
    "editorGroupHeader.tabsBackground": "#111316",
    "editorHoverWidget.background": "#111316",
    "editorHoverWidget.border": "#181a1f",
    "editorIndentGuide.activeBackground": "#ADADAD59",
    "editorIndentGuide.background": "#1D1F23",
    "editorWhitespace.foreground": "#393D44",
    "editorLineNumber.foreground": "#353B47",
    "editorMarkerNavigation.background": "#1B1E23",
    "editorRuler.foreground": "#9298A326",
    "editorSuggestWidget.background": "#1B1E23",
    "editorSuggestWidget.border": "#181a1f",
    "editorSuggestWidget.selectedBackground": "#2c313a",
    "editorWarning.foreground": "#B58558",
    "editorWidget.background": "#1B1E23",
    "focusBorder": "#2A2E38",
    "gitDecoration.ignoredResourceForeground": "#4E545E",
    "input.background": "#0A0B0C",
    "list.activeSelectionBackground": "#2c313a",
    "list.activeSelectionForeground": "#BCBFC4",
    "list.focusBackground": "#1D2026",
    "list.focusForeground": "#D6D6D6",
    "list.highlightForeground": "#AAAAAA",
    "list.hoverBackground": "#2c313a",
    "list.hoverForeground": "#E5E5E5",
    "list.inactiveSelectionBackground": "#1D2026",
    "list.inactiveSelectionForeground": "#BCBFC4",
    "list.warningForeground": "#B58558",

    "panelSectionHeader.background": "#1B1E23",
    "peekViewEditor.background": "#0E0F12",
    "peekViewEditor.matchHighlightBackground": "#1A1730",
    "peekViewResult.background": "#101214",
    "scrollbar.shadow": "#0E0F11",
    "scrollbarSlider.activeBackground": "#474D5980",
    "scrollbarSlider.background": "#31353F60",
    "scrollbarSlider.hoverBackground": "#3A404C80",

    "terminal.ansiBlack": "#3f4451",
    "terminal.ansiBlue": "#4aa5f0",
    "terminal.ansiCyan": "#8BE9FD",
    "terminal.ansiGreen": "#6CDD5F",
    "terminal.ansiMagenta": "#FF79C6",
    "terminal.ansiRed": "#e05561",
    "terminal.ansiWhite": "#F8F8F2",
    "terminal.ansiYellow": "#F1FA8C",
    "terminal.ansiBrightBlack": "#4f5666",
    "terminal.ansiBrightBlue": "#4dc4ff",
    "terminal.ansiBrightCyan": "#A4FFFF",
    "terminal.ansiBrightGreen": "#7CED71",
    "terminal.ansiBrightMagenta": "#FF92DF",
    "terminal.ansiBrightRed": "#ff616e",
    "terminal.ansiBrightWhite": "#FFFFFF",
    "terminal.ansiBrightYellow": "#FFFFA5",

    "terminal.background": "#111316",
    "terminal.border": "#2F343C",
    "terminal.foreground": "#A3ABB8",
    "terminal.selectionBackground": "#1B1E23"
  },
  "semanticTokenColors": {
    "keyword": {
      "foreground": "#bfdbfe"
    },
    "comment": {
      "foreground": "#798A9A",
      "fontStyle": "italic"
    },
    "punctuation": "#bfdbfe",
    "function": "#38bdf8",
    "method": "#38bdf8",
    "modifier": "#bfdbfe",
    "newOperator": "#bfdbfe",
    "operator": "#bfdbfe",
    "number": "#99f6e4",
    "string": "#34d399",
    "variable": "#93c5fd",
    "attribute": "#93c5fd",
    "property": "#93c5fd",
    "type": "#2dd4bf",
    "interface": "#2dd4bf",
    "selfTypeKeyword": {
      "foreground": "#2dd4bf",
      "fontStyle": "bold"
    },
    "class": "#06b6d4",
    "parameter": {
      "foreground": "#c7d2fe",
      "fontStyle": "italic"
    },
    "selfKeyword": "#c7d2fe",
    "namespace": "#c4b5fd",
    "enumMember": "#93c5fd",
    "function.defaultLibrary": "#22d3ee",
    "method.defaultLibrary": "#22d3ee",
    "class.defaultLibrary": "#22d3ee",
    "variable.defaultLibrary": "#7dd3fc",
    "property.defaultLibrary": "#7dd3fc",
    "*.defaultLibrary": {
      "fontStyle": "bold"
    }
  },
  "tokenColors": [
    {
      "name": "Comment",
      "scope": [
        "punctuation.definition.comment",
        "comment",
        "comment.block",
        "comment.block.documentation",
        "comment.line"
      ],
      "settings": {
        "foreground": "#798A9A",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Variables",
      "scope": [
        "variable",
        "variable.other",
        "support.other.variable",
        "string constant.other.placeholder"
      ],
      "settings": {
        "foreground": "#93c5fd"
      }
    },
    {
      "name": "Invalid",
      "scope": ["invalid", "invalid.illegal"],
      "settings": {
        "foreground": "#ffffff"
      }
    },
    {
      "name": "Storage",
      "scope": ["storage.type", "storage.modifier"],
      "settings": {
        "foreground": "#bfdbfe"
        // "fontStyle": "bold"
      }
    },
    {
      "name": "Types",
      "scope": [
        "storage.type.primitive",
        "entity.name.type",
        "support.type",
        "support.type.sys-types"
      ],
      "settings": {
        "foreground": "#2dd4bf",
        "fontStyle": ""
      }
    },
    {
      "name": "Keywords",
      "scope": [
        "keyword",
        "keyword.operator",
        "keyword.control",
        "keyword.operator.expression",
        "keyword.operator.new",
        "keyword.other",
        "variable.language.super",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#bfdbfe"
        // "fontStyle": "bold"
      }
    },
    {
      "name": "Braces",
      "scope": ["meta.brace"],
      "settings": {
        "foreground": "#cbd5e1"
      }
    },
    {
      "name": "Misc",
      "scope": [
        "constant.other.color",
        "punctuation",
        "punctuation.definition.tag",
        "punctuation.separator.inheritance.php",
        "punctuation.definition.tag.html",
        "punctuation.definition.tag.begin.html",
        "punctuation.definition.tag.end.html",
        "punctuation.section.embedded"
      ],
      "settings": {
        "foreground": "#bfdbfe"
      }
    },
    {
      "name": "Function, Special Method",
      "scope": [
        "entity.name.function",
        "meta.function-call",
        "variable.function",
        "support.function",
        "keyword.other.special-method"
      ],
      "settings": {
        "foreground": "#38bdf8"
      }
    },
    {
      "name": "Parameter",
      "scope": ["variable.parameter", "parameter", "variable.language.this"],
      "settings": {
        "foreground": "#a5b4fc"
      }
    },
    {
      "name": "Namespace / Module",
      "scope": ["entity.name.module", "support.module", "namespace"],
      "settings": {
        "foreground": "#c4b5fd"
      }
    },
    {
      "name": "Constants",
      "scope": [
        "variable.language",
        "constant.language",
        "support.constant",
        "constant.character",
        "constant.escape"
      ],
      "settings": {
        "foreground": "#77bbee"
      }
    },
    {
      "name": "Attributes",
      "scope": [
        "meta.object-literal.key",
        "variable.other.property",
        "variable.other.object.property",
        "variable.other.constant.property",
        "entity.other.attribute-name",
        "entity.name.record.field"
      ],
      "settings": {
        "foreground": "#93c5fd"
      }
    },
    {
      "name": "Enum Member",
      "scope": ["variable.other.enummember", "constant.other.enum"],
      "settings": {
        "foreground": "#93c5fd"
      }
    },
    {
      "name": "Number",
      "scope": ["constant.numeric"],
      "settings": {
        "foreground": "#99f6e4"
      }
    },
    {
      "name": "String, Symbols, Markup Heading",
      "scope": [
        "string",
        "punctuation.definition.string",
        "constant.other.symbol",
        "constant.other.key",
        "markup.heading",
        "markup.inserted.git_gutter"
      ],
      "settings": {
        "foreground": "#34d399"
      }
    },
    {
      "name": "Classes",
      "scope": [
        "entity.name.type.class",
        "support.class",
        "entity.other.inherited-class",
        "markup.changed.git_gutter"
      ],
      "settings": {
        "foreground": "#06b6d4"
      }
    },
    {
      "name": "Decorators",
      "scope": [
        "storage.type.annotation",
        "punctuation.decorator",
        "punctuation.definition.annotation"
      ],
      "settings": {
        "foreground": "#bfdbfe"
      }
    },
    {
      "name": "JSON Key",
      "scope": ["source.json punctuation", "support.type.property-name.json"],
      "settings": {
        "foreground": "#77bbee"
      }
    },
    /* Elm */
    {
      "name": "Type Constructors",
      "scope": ["constant.type-constructor.elm"],
      "settings": {
        "foreground": "#06b6d4"
      }
    },
    {
      "name": "Records",
      "scope": ["record.name.elm"],
      "settings": {
        "foreground": "#77bbee"
      }
    },
    {
      "name": "Values",
      "scope": ["meta.value.elm"],
      "settings": {
        "foreground": "#93c5fd"
      }
    },
    /* HTML CSS */
    {
      "name": "Tag",
      "scope": ["entity.name.tag", "meta.tag.sgml", "markup.deleted.git_gutter"],
      "settings": {
        "foreground": "#38bdf8"
      }
    },
    {
      "name": "CSS Class and Support",
      "scope": [
        "source.css support.type.property-name",
        "source.sass support.type.property-name",
        "source.scss support.type.property-name",
        "source.less support.type.property-name",
        "source.stylus support.type.property-name",
        "source.postcss support.type.property-name"
      ],
      "settings": {
        "foreground": "#93c5fd"
      }
    },
    {
      "name": "HTML Attributes",
      "scope": [
        "entity.other.attribute-name.html",
        "entity.other.attribute-name.jsx",
        "entity.other.attribute-name.tsx"
      ],
      "settings": {
        "foreground": "#a5b4fc"
      }
    },
    {
      "name": "CSS Classes",
      "scope": ["entity.other.attribute-name.class"],
      "settings": {
        "foreground": "#2dd4bf"
      }
    },
    {
      "name": "CSS ID's",
      "scope": ["source.sass keyword.control", "keyword.other.unit"],
      "settings": {
        "foreground": "#bfdbfe"
      }
    },
    /* Markdown */
    {
      "name": "Inserted",
      "scope": ["markup.inserted"],
      "settings": {
        "foreground": "#C3E88D"
      }
    },
    {
      "name": "Deleted",
      "scope": ["markup.deleted"],
      "settings": {
        "foreground": "#FF5370"
      }
    },
    {
      "name": "Changed",
      "scope": ["markup.changed"],
      "settings": {
        "foreground": "#C792EA"
      }
    },
    {
      "name": "Regular Expressions",
      "scope": ["string.regexp"],
      "settings": {
        "foreground": "#89DDFF"
      }
    },
    {
      "name": "Escape Characters",
      "scope": ["constant.character.escape"],
      "settings": {
        "foreground": "#89DDFF"
      }
    },
    {
      "name": "Markdown - Plain",
      "scope": ["text.html.markdown", "punctuation.definition.list_item.markdown"],
      "settings": {
        "foreground": "#EEFFFF"
      }
    },
    {
      "name": "Markdown - Markup Raw Inline",
      "scope": ["text.html.markdown markup.inline.raw.markdown"],
      "settings": {
        "foreground": "#C792EA"
      }
    },
    {
      "name": "Markdown - Markup Raw Inline Punctuation",
      "scope": [
        "text.html.markdown markup.inline.raw.markdown punctuation.definition.raw.markdown"
      ],
      "settings": {
        "foreground": "#65737E"
      }
    },
    {
      "name": "Markdown - Heading",
      "scope": [
        "markdown.heading",
        "markup.heading | markup.heading entity.name",
        "markup.heading.markdown punctuation.definition.heading.markdown"
      ],
      "settings": {
        "foreground": "#C3E88D"
      }
    },
    {
      "name": "Markup - Italic",
      "scope": ["markup.italic"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#f07178"
      }
    },
    {
      "name": "Markup - Bold",
      "scope": ["markup.bold", "markup.bold string"],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#f07178"
      }
    },
    {
      "name": "Markup - Bold-Italic",
      "scope": [
        "markup.bold markup.italic",
        "markup.italic markup.bold",
        "markup.quote markup.bold",
        "markup.bold markup.italic string",
        "markup.italic markup.bold string",
        "markup.quote markup.bold string"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#f07178"
      }
    },
    {
      "name": "Markup - Underline",
      "scope": ["markup.underline"],
      "settings": {
        "fontStyle": "underline",
        "foreground": "#F78C6C"
      }
    },
    {
      "name": "Markdown - Blockquote",
      "scope": ["markup.quote punctuation.definition.blockquote.markdown"],
      "settings": {
        "foreground": "#65737E"
      }
    },
    {
      "name": "Markup - Quote",
      "scope": ["markup.quote"],
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markdown - Link",
      "scope": ["string.other.link.title.markdown"],
      "settings": {
        "foreground": "#82AAFF"
      }
    },
    {
      "name": "Markdown - Link Description",
      "scope": ["string.other.link.description.title.markdown"],
      "settings": {
        "foreground": "#C792EA"
      }
    },
    {
      "name": "Markdown - Link Anchor",
      "scope": ["constant.other.reference.link.markdown"],
      "settings": {
        "foreground": "#FFCB6B"
      }
    },
    {
      "name": "Markup - Raw Block",
      "scope": ["markup.raw.block"],
      "settings": {
        "foreground": "#C792EA"
      }
    },
    {
      "name": "Markdown - Raw Block Fenced",
      "scope": ["markup.raw.block.fenced.markdown"],
      "settings": {
        "foreground": "#00000050"
      }
    },
    {
      "name": "Markdown - Fenced Bode Block",
      "scope": ["punctuation.definition.fenced.markdown"],
      "settings": {
        "foreground": "#00000050"
      }
    },
    {
      "name": "Markdown - Fenced Bode Block Variable",
      "scope": [
        "markup.raw.block.fenced.markdown",
        "variable.language.fenced.markdown",
        "punctuation.section.class.end"
      ],
      "settings": {
        "foreground": "#EEFFFF"
      }
    },
    {
      "name": "Markdown - Fenced Language",
      "scope": ["variable.language.fenced.markdown"],
      "settings": {
        "foreground": "#65737E"
      }
    },
    {
      "name": "Markdown - Separator",
      "scope": ["meta.separator"],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#65737E"
      }
    },
    {
      "name": "Markup - Table",
      "scope": ["markup.table"],
      "settings": {
        "foreground": "#EEFFFF"
      }
    }
  ]
}
